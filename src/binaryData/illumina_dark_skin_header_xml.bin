<?xml version="1.0"?>
<skin name="illumina_dark_skin_header" version="0.0">

	<!-- shared variables -->
	<variables>
		<variable key="asset" value="habbo_skin_illumina_dark_png" type="String"/>
	</variables>

	<!-- the following maps states to layouts and templates -->
	<states>
		<state name="default" layout="illumina_dark_header" template="illumina_dark_header_template"/>
	</states>

	<!-- templates defines regions in bitmap asset -->
	<templates>
		<template name="illumina_dark_header_template" asset="$asset">
			<entities>
				<entity name="top_left" type="bitmap"><region><Rectangle x="68" y="0" width="3" height="3"/></region></entity>
				<entity name="top_center" type="bitmap"><region><Rectangle x="71" y="0" width="32" height="3"/></region></entity>
				<entity name="top_right" type="bitmap"><region><Rectangle x="103" y="0" width="3" height="3"/></region></entity>
				<entity name="center_left" type="bitmap"><region><Rectangle x="68" y="3" width="3" height="24"/></region></entity>
				<entity name="center_center" type="bitmap"><region><Rectangle x="71" y="3" width="32" height="24"/></region></entity>
				<entity name="center_right" type="bitmap"><region><Rectangle x="103" y="3" width="3" height="24"/></region></entity>
				<entity name="bottom_left" type="bitmap"><region><Rectangle x="68" y="27" width="3" height="3"/></region></entity>
				<entity name="bottom_center" type="bitmap"><region><Rectangle x="71" y="27" width="32" height="3"/></region></entity>
				<entity name="bottom_right" type="bitmap"><region><Rectangle x="103" y="27" width="3" height="3"/></region></entity>
			</entities>
		</template>
	</templates>

	<!-- markup for layout drawer -->
	<layouts>
		<layout name="illumina_dark_header" transparent="true">
			<entities>
				<entity name="top_left"   id="0" colorize="false"><region><Rectangle x="0" y="12" width="3" height="3"/></region><scale horizontal="fixed" vertical="fixed"/></entity>
				<entity name="top_center"   id="0" colorize="false"><region><Rectangle x="3" y="12" width="32" height="3"/></region><scale horizontal="strech" vertical="fixed"/></entity>
				<entity name="top_right"   id="0" colorize="false"><region><Rectangle x="35" y="12" width="3" height="3"/></region><scale horizontal="move" vertical="fixed"/></entity>
				<entity name="center_left"   id="0" colorize="false"><region><Rectangle x="0" y="15" width="3" height="24"/></region><scale horizontal="fixed" vertical="strech"/></entity>
				<entity name="center_center"   id="0" colorize="false"><region><Rectangle x="3" y="15" width="32" height="24"/></region><scale horizontal="strech" vertical="strech"/></entity>
				<entity name="center_right"   id="0" colorize="false"><region><Rectangle x="35" y="15" width="3" height="24"/></region><scale horizontal="move" vertical="strech"/></entity>
				<entity name="bottom_left"   id="0" colorize="false"><region><Rectangle x="0" y="39" width="3" height="3"/></region><scale horizontal="fixed" vertical="move"/></entity>
				<entity name="bottom_center"   id="0" colorize="false"><region><Rectangle x="3" y="39" width="32" height="3"/></region><scale horizontal="strech" vertical="move"/></entity>
				<entity name="bottom_right"   id="0" colorize="false"><region><Rectangle x="35" y="39" width="3" height="3"/></region><scale horizontal="move" vertical="move"/></entity>
			</entities>
		</layout>
	</layouts>
</skin>