             <frame number="2" assetpartdefinition="sws"/>
             <frame number="1" assetpartdefinition="sws"/>
             <frame number="0" assetpartdefinition="sws"/>
         </part>

         <part set-type="ss">
             <frame number="0" assetpartdefinition="sws"/>
         </part>

         <part set-type="lhs">
             <frame number="0" assetpartdefinition="sws"/>
             <frame number="0" assetpartdefinition="sws"/>
             <frame number="1" assetpartdefinition="sws"/>
