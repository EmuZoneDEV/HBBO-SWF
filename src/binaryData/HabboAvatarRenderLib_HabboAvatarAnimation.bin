        </part>
        <part set-type="ls">
            <frame number="0" assetpartdefinition="wav"/>
        </part>
        <part set-type="lc">
            <frame number="0" assetpartdefinition="wav"/>
        </part>
    </action>

    <action id="Respect">
        <part set-type="lh">
            <frame number="0" assetpartdefinition="respect" repeats="15"/>
            <frame number="1" assetpartdefinition="respect" repeats="15"/>
        </part>
        <part set-type="ls">
            <frame number="0" assetpartdefinition="wav" repeats="15"/>
            <frame number="1" assetpartdefinition="wav" repeats="15"/>
        </part>
        <part set-type="lc">
            <frame number="0" assetpartdefinition="wav" repeats="15"/>
            <frame number="1" assetpartdefinition="wav" repeats="15"/>
        </part>
    </action>

	<action id="Blow">

         <part set-type="rh">
             <frame number="0" assetpartdefinition="blw" repeats="10"/>
             <frame number="1" assetpartdefinition="blw" repeats="10"/>
         </part>

        <part set-type="rs">
             <frame number="0" assetpartdefinition="drk"/>
        </part>

		<part set-type="rc">
			 <frame number="0" assetpartdefinition="drk"/>
		</part>

		<part set-type="ri">
			 <frame number="0" assetpartdefinition=""/>
		</part>

		<part set-type="ey">
			<frame number="0" assetpartdefinition="std" repeats="10"/>
			<frame number="0" assetpartdefinition="eyb" repeats="10"/>
		</part>

		<part set-type="fc">
			<frame number="0" assetpartdefinition="std" repeats="10"/>
			<frame number="0" assetpartdefinition="blw" repeats="10"/>
		</part>
    </action>

    <action id="Laugh">

         <part set-type="rh">
             <frame number="0" assetpartdefinition="blw"/>
         </part>

        <part set-type="rs">
             <frame number="0" assetpartdefinition="drk"/>
        </part>

        <part set-type="rc">
             <frame number="0" assetpartdefinition="drk"/>
        </part>

        <part set-type="ri">
             <frame number="0" assetpartdefinition=""/>
        </part>

        <part set-type="ey">
            <frame number="0" assetpartdefinition="std" repeats="2"/>
        </part>

        <part set-type="fc">
            <frame number="0" assetpartdefinition="sml"/>
        </part>

        <offsets>
            <frame id="0">
                <directions>
                    <direction id="0"><bodypart id="head" dx="0" dy="1"/></direction>
                    <direction id="1"><bodypart id="head" dx="0" dy="1"/></direction>
                    <direction id="2"><bodypart id="head" dx="0" dy="1"/></direction>
                    <direction id="3"><bodypart id="head" dx="0" dy="1"/></direction>
                    <direction id="4"><bodypart id="head" dx="0" dy="1"/></direction>
                    <direction id="5"><bodypart id="head" dx="0" dy="1"/></direction>
                    <direction id="6"><bodypart id="head" dx="0" dy="1"/></direction>
                    <direction id="7"><bodypart id="head" dx="0" dy="1"/></direction>
                </directions>
            </frame>

            <frame id="1">
                <directions>
                    <direction id="0"><bodypart id="head" dx="0" dy="0"/></direction>
                    <direction id="1"><bodypart id="head" dx="0" dy="0"/></direction>
                    <direction id="2"><bodypart id="head" dx="0" dy="0"/></direction>
                    <direction id="3"><bodypart id="head" dx="0" dy="0"/></direction>
                    <direction id="4"><bodypart id="head" dx="0" dy="0"/></direction>
                    <direction id="5"><bodypart id="head" dx="0" dy="0"/></direction>
                    <direction id="6"><bodypart id="head" dx="0" dy="0"/></direction>
                    <direction id="7"><bodypart id="head" dx="0" dy="0"/></direction>
                </directions>
            </frame>

        </offsets>
    </action>
	
	
